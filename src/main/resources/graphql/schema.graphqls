type User {
    id: ID!
    name: String!
    age: Int
    email: String!
}

type Book {
    id: ID!
    name: String!
    category: String
    quantity: Int!
    price: Int!
    author: User!
    status: BookStatus!
}

enum BookStatus {
    START,
    SELLING,
    SOLD_OUT
}

type Query {
    findAllUser: [User]!
    findAllBook: [Book]!
}
